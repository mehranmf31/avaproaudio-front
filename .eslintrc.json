{
  "env": {
      "browser": true,
      "es2021": true,
      "node": true
  },
  "extends": [
      "standard-with-typescript",
      "next",
      "plugin:@next/next/recommended"
  ],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
      "ecmaVersion": "latest",
      "sourceType": "module",
      "project": "./tsconfig.json"
  },
  "plugins": [
      "react",
      "@typescript-eslint"
  ],
  "rules": {
      "@typescript-eslint/explicit-function-return-type": "off"
  }
}
